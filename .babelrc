{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "electron": "22.0.0"
        },
        "modules": false,
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-class-properties",
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "react-refresh/babel"
      ]
    }
  }
}